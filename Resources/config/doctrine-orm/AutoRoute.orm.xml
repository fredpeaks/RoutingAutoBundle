<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          http://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">

    <entity name="Symfony\Cmf\Bundle\RoutingAutoBundle\Doctrine\Orm\AutoRoute">
        <id name="id" type="integer" column="id">
            <generator strategy="AUTO"/>
        </id>

        <field name="name" type="string" unique="true"/>
        <field name="position" type="integer"/>
        <field name="contentCode" type="string" nullable="true"/>
        <many-to-one field="redirectRoute" target-entity="Symfony\Cmf\Bundle\RoutingAutoBundle\Doctrine\Orm\AutoRoute"/>
    </entity>

</doctrine-mapping>
